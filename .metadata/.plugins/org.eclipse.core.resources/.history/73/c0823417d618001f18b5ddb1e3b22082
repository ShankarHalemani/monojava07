package com.techlabs.model;

public class Rectangle {
	private int width;
	private int height;

	public Rectangle() {
		System.out.println("Rectangle details");
	}

	public Rectangle(int rwidth, int rheight) {
		width = rwidth;
		height = rheight;
	}

	public int getWidth() {
		return width;
	}

	public int getHeight() {
		return height;
	}

	public int getArea() {
		return width * height;
	}
}
