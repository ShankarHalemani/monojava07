package com.techlabs.test;

import com.techlabs.model.Complex;

public class ComplexTest {

	public static void main(String[] args) {
		Complex complex1 = new Complex(12,13);
		Complex complex2 = new Complex(2,3);
		
		Complex complex3 = new Complex();
		complex3.getAddition(complex1, complex2);
		System.out.println("Addition of complex number");
		System.out.println("Complex number : "+complex3.getReal()+"+"+complex3.getImaginary()+"i");

	}

}
